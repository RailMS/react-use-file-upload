{
  "name": "@railms/react-use-file-upload",
  "version": "0.9.5f5",
  "description": "A React Hook to make dealing with file uploading easier.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
  "build:cjs": "tsc --project tsconfig.cjs.json",
  "build:esm": "tsc --project tsconfig.esm.json",
  "build": "npm run build:cjs && npm run build:esm && mv dist/esm/index.js dist/index.esm.js",
    "dev": "tsc -w",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js --watchAll",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RailMS/react-use-file-upload.git"
  },
  "keywords": [
    "react file upload",
    "react hook file upload",
    "upload file",
    "file uploading",
    "use file upload"
  ],
  "author": "Daniel Zuzevich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RailMS/react-use-file-upload/issues"
  },
  "homepage": "https://github.com/RailMS/react-use-file-upload#readme",
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^26.0.15",
    "@types/react": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.6.0",
    "prettier": "^2.3.0",
    "react": "^18.x",
    "react-test-renderer": "^18.x",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "react": "^18.x",
    "react-dom": "^18.x"
  },
  "files": [
    "dist"
  ]
}
